<meta property="og:url" content="https://webhint.io" />
<meta property="og:type" content="website" />
<meta property="og:image" content="https://webhint.io<% if (page && page.image) { %><%= page.image %><% } else { %>/images/apple-touch-icon.png<% } %>" />
