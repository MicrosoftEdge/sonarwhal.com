<meta property="og:image" content="{{setDefault page.image '/images/apple-touch-icon.png'}}" />
<meta property="og:description" content="{{setDefault page.description 'sonarwhal, a linting tool for the web focused on best practices and flexibility for the end user'}}" />
<meta property="og:title" content="{{page.title}}" />

<meta name="description" content="{{setDefault page.description 'sonarwhal, a linting tool for the web focused on best practices and flexibility for the end user'}}">
<meta name="viewport" content="width=device-width">

<meta name="theme-color" content="#4046dd">
<link rel="manifest" href="/site.webmanifest">

<link rel="apple-touch-icon" href="/images/apple-touch-icon.png">

{{#if status}}
    {{#if (isPass status)}}
        <link rel="icon" type="image/png" class="favicon" href="/images/favicon_passed.ico" sizes="32x32">
    {{else}}
            {{#if (isError status)}}
                <link rel="icon" type="image/png" class="favicon" href="/images/favicon_failed.ico" sizes="32x32">
            {{else}}
                <link rel="icon" type="image/png" class="favicon" href="/images/favicon_pending.ico" sizes="32x32">
            {{/if}}
    {{/if}}
{{else}}
    <link rel="icon" type="image/png" class="favicon" href="/images/favicon-32x32.png" sizes="32x32">
{{/if}}

