<% if (result && result.status) { %>
    <% if (result.status === 'error' || result.errors !== 0 || result.warnings !== 0) { %>
        <link rel="icon" href="/images/favicon_failed.ico">
    <% } else if (result.status === 'finished') { %>
        <link rel="icon" href="/images/favicon_passed.ico">
    <% } else { %>
        <link rel="icon" href="/images/favicon_pending.ico">
    <% } %>
<% } else { %>
    <link rel="icon" href="/images/favicon.ico">
<% } %>
